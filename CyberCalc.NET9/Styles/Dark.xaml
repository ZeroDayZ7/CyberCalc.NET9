<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="CyberPrimary">#555555</Color>
    <Color x:Key="CyberSecondary">#AAAAAA</Color>
    <Color x:Key="CyberAccent">#cf5d1b</Color>
    <Color x:Key="CyberBackground">#1C2526</Color>
    <SolidColorBrush x:Key="WindowBackground"
                     Color="{StaticResource CyberBackground}" />
    <SolidColorBrush x:Key="ButtonBackground"
                     Color="#2F4F4F" />
    <SolidColorBrush x:Key="ButtonForeground"
                     Color="#FFFFFF" />
    <SolidColorBrush x:Key="BorderBrush"
                     Color="{StaticResource CyberPrimary}" />
    <SolidColorBrush x:Key="TextColor"
                     Color="#FFFFFF" />
    <SolidColorBrush x:Key="CyberSecondaryBrush"
                     Color="{StaticResource CyberSecondary}" />
    <SolidColorBrush x:Key="CyberAccentBrush"
                     Color="{StaticResource CyberAccent}" />
</ResourceDictionary>